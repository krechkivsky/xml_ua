	</xsd:element>
	<xsd:element name="ParcelMetricInfo">
		<xsd:annotation>
			<xsd:documentation>Метрична інформація земельної ділянки, обмежень її використання та угідь</xsd:documentation>
			<xsd:appinfo>Метрична інформація ⋮</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ParcelID" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Унікальний номер ділянки в межах кварталу</xsd:documentation>
						<xsd:appinfo>ParcelID</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{4}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Description" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Опис земельної ділянки</xsd:documentation>
						<xsd:appinfo>Опис</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Area" minOccurs="1" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Площа земельної ділянки</xsd:documentation>
						<xsd:appinfo>Площа ділянки</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="MeasurementUnit">
								<xsd:annotation>
									<xsd:documentation>Одиниці виміру площі</xsd:documentation>
									<xsd:appinfo>Одиниці виміру ⋮</xsd:appinfo>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="га"/>
										<xsd:enumeration value="кв.м"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="Size" type="xsd:double">
								<xsd:annotation>
									<xsd:documentation>Площа</xsd:documentation>
									<xsd:appinfo>Площа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DeterminationMethod" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Метод визначення площі земельної ділянки</xsd:documentation>
									<xsd:appinfo>Метод визначення ⋮</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:choice>
										<xsd:element name="ExhangeFileCoordinates" type="xsd:string" fixed="">
											<xsd:annotation>
												<xsd:documentation>За координатами обмінного файлу</xsd:documentation>
												<xsd:appinfo>За координатами</xsd:appinfo>
											</xsd:annotation>
