					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Document" type="PropertyAcquisitionJustificationDocument">
								<xsd:annotation>
									<xsd:documentation>Назва документа (код)</xsd:documentation>
									<xsd:appinfo>Назва документа ↓</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DocumentDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата прийняття (укладання) документа</xsd:documentation>
									<xsd:appinfo>Дата документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DocumentNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Номер</xsd:documentation>
									<xsd:appinfo>Номер документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalAuthority" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Орган, який прийняв рішення</xsd:documentation>
									<xsd:appinfo>Орган</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="MarkInfo" minOccurs="0" maxOccurs="unbounded">
