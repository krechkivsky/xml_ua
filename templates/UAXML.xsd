						<xsd:documentation>Рішення про передачу земельної ділянки в оренду</xsd:documentation>
						<xsd:appinfo>Рішення</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="ExecutivePower" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Назва органу</xsd:documentation>
									<xsd:appinfo>Назва органу</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalDocument" type="PropertyAcquisitionJustificationDocument">
								<xsd:annotation>
									<xsd:documentation>Вид документа</xsd:documentation>
									<xsd:appinfo>Вид документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalDocumentName" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Назва документа</xsd:documentation>
									<xsd:appinfo>Назва документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата прийняття документа</xsd:documentation>
									<xsd:appinfo>Дата прийняття</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalDocumentNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Номер документа</xsd:documentation>
									<xsd:appinfo>Номер документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="LeaseAgreement" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Інформація про договір оренди земельної ділянки</xsd:documentation>
						<xsd:appinfo>Договір оренди</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="Leasees">
								<xsd:annotation>
									<xsd:documentation>Блок опису усіх орендарів</xsd:documentation>
									<xsd:appinfo>Орендарі</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Leasee" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Дані орендаря</xsd:documentation>
												<xsd:appinfo>Орендар</xsd:appinfo>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:choice>
													<xsd:element name="NaturalPerson" type="NaturalPersonInfo">
														<xsd:annotation>
															<xsd:documentation>Дані про фізичну особу</xsd:documentation>
															<xsd:appinfo>Фізична особа</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="LegalEntity" type="LegalEntityInfo">
														<xsd:annotation>
															<xsd:documentation>Дані про юридичну особу</xsd:documentation>
															<xsd:appinfo>Юридична особа</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
												</xsd:choice>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Area" type="xsd:double">
								<xsd:annotation>
									<xsd:documentation>Площа земельної ділянки або її частини, наданої в оренду</xsd:documentation>
									<xsd:appinfo>Площа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="LeaseTerm">
								<xsd:annotation>
									<xsd:documentation>Строк оренди</xsd:documentation>
									<xsd:appinfo>Строк оренди</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="StartDate" type="xsd:date" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Початок оренди</xsd:documentation>
												<xsd:appinfo>Початок</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="ExpirationDate" type="xsd:date" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Закінчення оренди</xsd:documentation>
												<xsd:appinfo>Закінчення</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="LeaseDuration" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Строк дії оренди</xsd:documentation>
												<xsd:appinfo>Строк дії</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Rent">
								<xsd:annotation>
									<xsd:documentation>Орендна плата</xsd:documentation>
									<xsd:appinfo>Орендна плата</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="MoneyRent" type="xsd:double">
											<xsd:annotation>
												<xsd:documentation>Розмір орендної плати за земельну ділянку або її частину у грошовій формі</xsd:documentation>
												<xsd:appinfo>Грошова форма</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="OtherRent" type="xsd:string" minOccurs="0" maxOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Розмір орендної плати за земельну ділянку або її частину, надану в оренду в інших формах плати</xsd:documentation>
												<xsd:appinfo>Інша форма</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="RegistrationNumber">
								<xsd:annotation>
									<xsd:documentation>Реєстраційний номер державної реєстрації договору оренди землі</xsd:documentation>
									<xsd:appinfo>Реєстраційний номер</xsd:appinfo>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[0-9]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="RegistrationDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата державної реєстрації договору оренди землі</xsd:documentation>
									<xsd:appinfo>Дата реєстрації</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Externals">
					<xsd:annotation>
						<xsd:documentation>Блок опису зовнішніх полігонів оренди земельної ділянки</xsd:documentation>
						<xsd:appinfo>Межі ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Межа оренди земельної ділянки</xsd:documentation>
									<xsd:appinfo>Межа зовнішнього полігона</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Internals" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Блок опису внутрішніх полігонів оренди земельної ділянки</xsd:documentation>
									<xsd:appinfo>Внутрішні межі</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Межа внутрішнього полігона оренди земельної ділянки</xsd:documentation>
												<xsd:appinfo>Межа внутрішнього полігона</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SubleaseInfo">
		<xsd:annotation>
			<xsd:documentation>Суборенда земельної ділянки</xsd:documentation>
			<xsd:appinfo>Суборенда</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Subleasees">
					<xsd:annotation>
						<xsd:documentation>Блок опису усіх cуборендарів</xsd:documentation>
						<xsd:appinfo>Суборендарі</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Subleasee" minOccurs="1" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Дані cуборендаря</xsd:documentation>
									<xsd:appinfo>Суборендар</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:choice>
										<xsd:element name="NaturalPerson" type="NaturalPersonInfo">
											<xsd:annotation>
												<xsd:documentation>Дані про фізичну особу</xsd:documentation>
												<xsd:appinfo>Фізична особа</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="LegalEntity" type="LegalEntityInfo">
											<xsd:annotation>
												<xsd:documentation>Дані про юридичну особу</xsd:documentation>
												<xsd:appinfo>Юридична особа</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:choice>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Area" type="xsd:double">
					<xsd:annotation>
						<xsd:documentation>Площа земельної ділянки або її частини</xsd:documentation>
						<xsd:appinfo>Площа</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RegistrationDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Дата державної реєстрації договору суборенди землі</xsd:documentation>
						<xsd:appinfo>Дата реєстрації</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RegistrationNumber">
					<xsd:annotation>
						<xsd:documentation>Реєстраційний номер договору суборенди землі</xsd:documentation>
						<xsd:appinfo>Реєстраційний номер</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Subrent">
