		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="CadastralQuarterNumber" default="000">
					<xsd:annotation>
						<xsd:documentation>Номер кадастрового кварталу</xsd:documentation>
						<xsd:appinfo>Номер</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{3}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="MSK" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Код регіону (перші 2 символи коду КОАТУУ)</xsd:documentation>
						<xsd:appinfo>Код регіону</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{2}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="RegionalContacts" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Інформація про регіон</xsd:documentation>
						<xsd:appinfo>Контакти ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="LocalAuthorityHead" type="FullNameType"  minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Прізвище, ім’я та по батькові керівника органу виконавчої влади або місцевого самоврядування</xsd:documentation>
									<xsd:appinfo>ПІБ керівника місцевої влади ⋮</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DKZRHead" type="FullNameType" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Прізвище, ім’я та по батькові начальника територіального органу земельних ресурсів</xsd:documentation>
									<xsd:appinfo>ПІБ начальника ДКЗР ⋮</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Externals" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Блок опису зовнішніх полігонів кадастрового кварталу</xsd:documentation>
						<xsd:appinfo>Межі ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Межа зовнішнього полігона кадастрового кварталу</xsd:documentation>
									<xsd:appinfo>Межа зовнішнього полігона</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Internals" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Блок опису внутрішніх полігонів кадастрового кварталу</xsd:documentation>
									<xsd:appinfo>Внутрішні межі</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Межа внутрішнього полігона кадастрового кварталу</xsd:documentation>
												<xsd:appinfo>Межа внутрішнього полігона</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Parcels" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Блок опису земельних ділянок</xsd:documentation>
						<xsd:appinfo>Блок земельних ділянок ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="ParcelInfo" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="CadastralZoneInfo">
		<xsd:annotation>
			<xsd:documentation>Кадастрова зона</xsd:documentation>
			<xsd:appinfo>Кадастрова зона ⋮</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="CadastralZoneNumber">
					<xsd:annotation>
						<xsd:documentation>Номер кадастрової зони</xsd:documentation>
						<xsd:appinfo>Номер кадастрової зони</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{10}:[0-9]{2}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="CadastralUnit" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Ознака кадастрової одиниці</xsd:documentation>
						<xsd:appinfo>Ознака кадастрової одиниці</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="CadastralZone" type="xsd:string" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Кадастрова зона</xsd:documentation>
									<xsd:appinfo>Ознака</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Externals" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Блок опису зовнішніх полігонів кадастрової зони</xsd:documentation>
						<xsd:appinfo>Межі ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Межа зовнішнього полігона кадастрової зони</xsd:documentation>
									<xsd:appinfo>Межа зовнішнього полігона</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Internals" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Блок опису внутрішніх полігонів кадастрової зони</xsd:documentation>
									<xsd:appinfo>Внутрішні межі</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Межа внутрішнього полігона кадастрової зони</xsd:documentation>
												<xsd:appinfo>Межа внутрішнього полігона</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="CadastralQuarters" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Блок опису усіх кадастрових кварталів</xsd:documentation>
						<xsd:appinfo>Блок кадастрових кварталів ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="CadastralQuarterInfo" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="UkrainianCadastralExchangeFile">
		<xsd:annotation>
			<xsd:documentation>Кореневий елемент</xsd:documentation>
			<xsd:appinfo>Обмінний файл ⋮</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="AdditionalPart">
					<xsd:annotation>
						<xsd:documentation>Інформація про реквізити обмінного файлу та виконавця робіт</xsd:documentation>
						<xsd:appinfo>Службова частина ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="ServiceInfo"/>
							<xsd:element ref="InfoLandWork"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="InfoPart">
					<xsd:annotation>
						<xsd:documentation>Інформація про земельно-кадастрові одиниці, територіальні зони і їх метричні відомості</xsd:documentation>
						<xsd:appinfo>Інформаційна частина ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="MetricInfo"/>
							<xsd:element ref="CadastralZoneInfo" minOccurs="0" maxOccurs="unbounded"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>