									<xsd:documentation>Дані про фізичну особу</xsd:documentation>
									<xsd:appinfo>Фізична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="EntitlementDocument" maxOccurs="unbounded">
