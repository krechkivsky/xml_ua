	</xsd:element>
	<xsd:element name="LeaseInfo">
		<xsd:annotation>
			<xsd:documentation>Оренда земельної ділянки</xsd:documentation>
			<xsd:appinfo>Оренда</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ExecutivePowerDecision" minOccurs="0" maxOccurs="unbounded">
