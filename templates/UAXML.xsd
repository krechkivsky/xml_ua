												<xsd:documentation>Відомості про осіб, які підписали державний акт</xsd:documentation>
												<xsd:appinfo>ПІБ того, хто підписав</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="SignedByPosition" type="xsd:string" maxOccurs="2">
											<xsd:annotation>
												<xsd:documentation>Посади осіб</xsd:documentation>
												<xsd:appinfo>Посада того, хто підписав</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RegisteredBy" type="FullNameType">
											<xsd:annotation>
												<xsd:documentation>Відомості про особу, яка зареєструвала державний акт</xsd:documentation>
												<xsd:appinfo>ПІБ того, хто зареєстрував</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RegisteredByPosition" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Посада особи</xsd:documentation>
												<xsd:appinfo>Посада того, хто зареєстрував</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="DeliveryDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Дата видачі державного акта</xsd:documentation>
						<xsd:appinfo>Дата видачі</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DeliveryPerson">
					<xsd:annotation>
						<xsd:documentation>Відомості про особу яка отримала державний акт</xsd:documentation>
