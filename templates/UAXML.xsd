						<xsd:documentation>Плата за суборенду</xsd:documentation>
						<xsd:appinfo>Плата за суборенду</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="MoneySubrent" type="xsd:double">
