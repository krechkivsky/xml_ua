			<xsd:appinfo>Технічна документація ⋮</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="DocumentationType" type="DocumentationType">
					<xsd:annotation>
						<xsd:documentation>Вид документації</xsd:documentation>
						<xsd:appinfo>Вид документації ↓</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="DraftingDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Дата складання документації</xsd:documentation>
						<xsd:appinfo>Дата складання ↓</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RegistrationData" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Реєстраційні дані документації</xsd:documentation>
