									<xsd:documentation>Строк дії суборенди</xsd:documentation>
									<xsd:appinfo>Строк дії</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Externals">
					<xsd:annotation>
						<xsd:documentation>Блок опису зовнішніх полігонів суборенди земельної ділянки</xsd:documentation>
						<xsd:appinfo>Межі ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Межа суборенди земельної  ділянки</xsd:documentation>
									<xsd:appinfo>Межа зовнішнього полігона</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Internals" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Блок опису внутрішніх полігонів суборенди земельної ділянки</xsd:documentation>
									<xsd:appinfo>Внутрішні межі</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Межа внутрішнього полігона суборенди земельної ділянки</xsd:documentation>
												<xsd:appinfo>Межа внутрішнього полігона</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="RestrictionInfo">
		<xsd:annotation>
			<xsd:documentation>Земельний сервітут, емфітевзис, суперфіцій та інші обмеження земельної ділянки</xsd:documentation>
			<xsd:appinfo>Обмеження</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="RestrictionCode" type="RestrictionCode">
					<xsd:annotation>
						<xsd:documentation>Код обмеження</xsd:documentation>
						<xsd:appinfo>Код обмеження ↓</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RestrictionName" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Зміст обмеження</xsd:documentation>
						<xsd:appinfo>Зміст обмеження</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Externals">
					<xsd:annotation>
						<xsd:documentation>Блок опису зовнішніх полігонів зони обмеження</xsd:documentation>
						<xsd:appinfo>Межі ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Межа зони обмеження</xsd:documentation>
									<xsd:appinfo>Межа зовнішнього полігона</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Internals" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Блок опису внутрішніх полігонів зони обмеження</xsd:documentation>
									<xsd:appinfo>Внутрішні межі</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Межа внутрішнього полігона зони обмеження</xsd:documentation>
												<xsd:appinfo>Межа внутрішнього полігона</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RestrictionEntitlement">
					<xsd:annotation>
						<xsd:documentation>Підстава для виникнення права земельного сервітуту, емфітевзису, суперфіція та інших обмежень щодо користування земельною ділянкою</xsd:documentation>
						<xsd:appinfo>Підстава обмеження</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="DocumentType" type="RestrictionEntitlementDocumentType">
								<xsd:annotation>
									<xsd:documentation>Вид документа, на підставі якого виникає право</xsd:documentation>
									<xsd:appinfo>Вид документа ↓</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DocumentName" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Назва документа, на підставі якого виникає право</xsd:documentation>
									<xsd:appinfo>Назва документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="DocumentDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата прийняття (укладення) документа</xsd:documentation>
									<xsd:appinfo>Дата прийняття</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="NaturalPerson" type="NaturalPersonInfo">
								<xsd:annotation>
									<xsd:documentation>Дані про фізичну особу</xsd:documentation>
									<xsd:appinfo>Фізична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="LegalEntity" type="LegalEntityInfo">
								<xsd:annotation>
									<xsd:documentation>Дані про юридичну особу</xsd:documentation>
									<xsd:appinfo>Юридична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RestrictionTerm">
					<xsd:annotation>
						<xsd:documentation>Термін дії</xsd:documentation>
						<xsd:appinfo>Термін дії</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice>
							<xsd:element name="Permanent" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Постійний</xsd:documentation>
									<xsd:appinfo>Постійний</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Time">
								<xsd:annotation>
									<xsd:documentation>Строковий</xsd:documentation>
									<xsd:appinfo>Строковий</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="StartDate" type="xsd:date">
											<xsd:annotation>
												<xsd:documentation>Початок дії</xsd:documentation>
												<xsd:appinfo>Початок</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="ExpirationDate" type="xsd:date">
											<xsd:annotation>
												<xsd:documentation>Закінчення дії</xsd:documentation>
												<xsd:appinfo>Закінчення</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="Duration" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Строк дії</xsd:documentation>
												<xsd:appinfo>Строк дії</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RegistrationDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Дата реєстрації обмеження</xsd:documentation>
						<xsd:appinfo>Дата реєстрації</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="RegistrationNumber">
					<xsd:annotation>
						<xsd:documentation>Реєстраційний номер обмеження</xsd:documentation>
						<xsd:appinfo>Реєстраційний номер</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Beneficiaries">
					<xsd:annotation>
						<xsd:documentation>Блок опису усіх осіб, на користь яких встановлено обмеження</xsd:documentation>
						<xsd:appinfo>Бенефіціарій</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Beneficiary" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Особа, на користь якої встановлено обмеження</xsd:documentation>
									<xsd:appinfo>Бенефіціар</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:choice>
										<xsd:element name="NaturalPerson" type="NaturalPersonInfo">
											<xsd:annotation>
												<xsd:documentation>Дані про фізичну особу</xsd:documentation>
												<xsd:appinfo>Фізична особа</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="LegalEntity" type="LegalEntityInfo">
											<xsd:annotation>
												<xsd:documentation>Дані про юридичну особу</xsd:documentation>
												<xsd:appinfo>Юридична особа</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:choice>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Payment">
					<xsd:annotation>
						<xsd:documentation>Плата за користування земельною ділянкою</xsd:documentation>
						<xsd:appinfo>Оплата</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice>
							<xsd:element name="Paid">
								<xsd:annotation>
									<xsd:documentation>Платний</xsd:documentation>
									<xsd:appinfo>Платний</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:all>
										<xsd:element name="MoneyPayment" type="xsd:double">
											<xsd:annotation>
												<xsd:documentation>Розмір плати за земельну ділянку або її частину у грошовій формі</xsd:documentation>
												<xsd:appinfo>Грошова форма</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="OtherPayment" type="xsd:string" minOccurs="0" maxOccurs="1">
											<xsd:annotation>
												<xsd:documentation>Розмір плати за земельну ділянку або її частину, надану в суборенду у інших формах плати</xsd:documentation>
												<xsd:appinfo>Інша форма</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:all>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Free" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Безоплатний</xsd:documentation>
									<xsd:appinfo>Безоплатний</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="SENumber" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Обліковий номер частини земельної ділянки, на яку поширюється дія сервітуту</xsd:documentation>
						<xsd:appinfo>Обліковий номер частини земельної ділянки</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{10}:[0-9]{2}:[0-9]{3}:[0-9]{4}:[0-9]{4}:[0-9]{8}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="LandParcelInfo">
		<xsd:annotation>
			<xsd:documentation>Угіддя земельної ділянки</xsd:documentation>
			<xsd:appinfo>Угіддя</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="CadastralCode" type="xsd:string" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Кадастровий номер</xsd:documentation>
						<xsd:appinfo>Кадастровий номер</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="LandCode" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Код угіддя</xsd:documentation>
						<xsd:appinfo>Код угіддя</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="MetricInfo">
					<xsd:annotation>
						<xsd:documentation>Метрична інформація</xsd:documentation>
						<xsd:appinfo>Метрична інформація ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Area" minOccurs="1" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Площа угіддя</xsd:documentation>
									<xsd:appinfo>Площа угіддя</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="MeasurementUnit">
											<xsd:annotation>
												<xsd:documentation>Одиниці виміру площі</xsd:documentation>
												<xsd:appinfo>Одиниці виміру ⋮</xsd:appinfo>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:enumeration value="га"/>
													<xsd:enumeration value="кв.м"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:element>
										<xsd:element name="Size" type="xsd:double">
											<xsd:annotation>
												<xsd:documentation>Значення площі</xsd:documentation>
												<xsd:appinfo>Площа</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="DeterminationMethod" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Метод визначення площі земельної ділянки</xsd:documentation>
												<xsd:appinfo>Метод визначення ⋮</xsd:appinfo>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:choice>
													<xsd:element name="ExhangeFileCoordinates" type="xsd:string" fixed="">
														<xsd:annotation>
															<xsd:documentation>За координатами обмінного файлу</xsd:documentation>
															<xsd:appinfo>За координатами</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="DocExch" type="xsd:string" fixed="">
														<xsd:annotation>
															<xsd:documentation>Згідно із правовстановлювальним документом</xsd:documentation>
															<xsd:appinfo>За документом</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="Calculation">
														<xsd:annotation>
															<xsd:documentation>Переобчислення</xsd:documentation>
															<xsd:appinfo>Переобчислення</xsd:appinfo>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:sequence>
																<xsd:element ref="CoordinateSystem"/>
															</xsd:sequence>
														</xsd:complexType>
													</xsd:element>
												</xsd:choice>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Perimeter" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Периметр угіддя</xsd:documentation>
									<xsd:appinfo>Периметр</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Error" type="xsd:double" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Середня квадратична похибка обчислення площі угіддя</xsd:documentation>
									<xsd:appinfo>Похибка</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Externals">
								<xsd:annotation>
									<xsd:documentation>Блок опису зовнішніх полігонів угіддя</xsd:documentation>
									<xsd:appinfo>Межі ⋮</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Межа зовнішнього полігона угіддя</xsd:documentation>
												<xsd:appinfo>Межа зовнішнього полігона</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="Internals" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Блок опису внутрішніх полігонів угідь земельної ділянки</xsd:documentation>
												<xsd:appinfo>Внутрішні межі</xsd:appinfo>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element name="Boundary" type="Boundary" maxOccurs="unbounded">
														<xsd:annotation>
															<xsd:documentation>Межа внутрішнього полігона угіддя земельної ділянки</xsd:documentation>
															<xsd:appinfo>Межа внутрішнього полігона</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AdjacentUnitInfo">
		<xsd:annotation>
			<xsd:documentation>Суміжник кадастрової одиниці</xsd:documentation>
			<xsd:appinfo>Суміжник</xsd:appinfo>
		</xsd:annotation>
