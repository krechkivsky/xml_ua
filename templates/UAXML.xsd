						<xsd:documentation>Ознака конфлікту меж</xsd:documentation>
						<xsd:appinfo>Конфлікт меж</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Proprietor" minOccurs="1" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Власник або розпорядник</xsd:documentation>
						<xsd:appinfo>Власник ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice>
							<xsd:element name="NaturalPerson" type="NaturalPersonInfo2">
								<xsd:annotation>
									<xsd:documentation>Дані про фізичну особу</xsd:documentation>
									<xsd:appinfo>Фізична особа ⋮</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="LegalEntity" type="LegalEntityInfo2">
								<xsd:annotation>
									<xsd:documentation>Дані про юридичну особу</xsd:documentation>
									<xsd:appinfo>Юридична особа ⋮</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="AdditionalInfoBlock" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Блок додаткової інформації</xsd:documentation>
						<xsd:appinfo>Блок додаткової інформації ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AdditionalInfo" type="xsd:string" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Додаткова інформація</xsd:documentation>
									<xsd:appinfo>Додаткова інформація</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
