						<xsd:appinfo>Блок додаткової інформації ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="AdditionalInfo" type="xsd:string" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>Додаткова інформація</xsd:documentation>
									<xsd:appinfo>Додаткова інформація</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
