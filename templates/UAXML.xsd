									<xsd:documentation>Дані про фізичну особу</xsd:documentation>
									<xsd:appinfo>Фізична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="LegalEntity" type="LegalEntityInfo">
								<xsd:annotation>
									<xsd:documentation>Дані про юридичну особу</xsd:documentation>
									<xsd:appinfo>Юридична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Grantor" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Особа, яка надає в користування земельну ділянку</xsd:documentation>
						<xsd:appinfo>Особа, яка надає права</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:choice>
							<xsd:element name="NaturalPerson" type="NaturalPersonInfo">
								<xsd:annotation>
									<xsd:documentation>Дані про фізичну особу</xsd:documentation>
									<xsd:appinfo>Фізична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="LegalEntity" type="LegalEntityInfo">
								<xsd:annotation>
									<xsd:documentation>Дані про юридичну особу</xsd:documentation>
									<xsd:appinfo>Юридична особа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="TechnicalDocumentationInfo">
		<xsd:annotation>
