												<xsd:documentation>Найменування нотаріальної контори, назва нотаріального округу та (або) прізвище, ім’я, по батькові нотаріуса</xsd:documentation>
												<xsd:appinfo>Нотаріус</xsd:appinfo>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:choice>
													<xsd:element name="FullName" type="FullNameType">
														<xsd:annotation>
															<xsd:documentation>Дані про фізичну особу</xsd:documentation>
															<xsd:appinfo>ПІБ</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="LegalEntity" type="LegalEntityInfo">
														<xsd:annotation>
															<xsd:documentation>Дані про юридичну особу</xsd:documentation>
															<xsd:appinfo>Юридична особа</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
												</xsd:choice>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="NotaryMarkDate" type="xsd:date">
											<xsd:annotation>
												<xsd:documentation>Дата відмітки нотаріуса</xsd:documentation>
												<xsd:appinfo>Дата відмітки</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="NotaryMarkNumber" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Номер відмітки нотаріуса</xsd:documentation>
												<xsd:appinfo>Номер відмітки</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="RegistrationAuthorityMark">
