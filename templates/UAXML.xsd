<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" version="0.7">

	<xsd:simpleType name="DocumentList">
		<xsd:union>
			<xsd:simpleType>
				<xsd:restriction base="DocumentListEnumeration"/>
			</xsd:simpleType>
			<xsd:simpleType>
				<xsd:restriction base="DocumentNameType"/>
			</xsd:simpleType>
		</xsd:union>
	</xsd:simpleType>

	<xsd:simpleType name="DocumentNameType">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="DocumentListEnumeration">
		<xsd:annotation>
			<xsd:documentation>Перелік документів для формування обмінного файлу</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="DocumentationType">
		<xsd:annotation>
			<xsd:documentation>Види документації із землеустрою</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="3"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ExpertiseRequired">
		<xsd:annotation>
			<xsd:documentation>Форми державної експертизи землевпорядної документації</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Category">
		<xsd:annotation>
			<xsd:documentation>Класифікатор категорій земель</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OwnershipInfoCode">
		<xsd:annotation>
			<xsd:documentation>Форми власності на земельну ділянку</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PropertyAcquisitionJustificationDocument">
		<xsd:annotation>
			<xsd:documentation>Перелік підстав набуття прав на земельну ділянку</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LegalModeType">
		<xsd:annotation>
			<xsd:documentation>Права користування земельною ділянкою</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ExpertOpinion">
		<xsd:annotation>
			<xsd:documentation>Типи висновків експертизи землевпорядної документації</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="NonEmptyString">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="StateActType">
		<xsd:annotation>
			<xsd:documentation>Типи державного акта</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RestrictionCode">
		<xsd:annotation>
			<xsd:documentation>Класифікатор обмежень та обтяжень у використанні земельних ділянок</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RestrictionEntitlementDocumentType">
		<xsd:annotation>
			<xsd:documentation>Підстави встановлення обмеження використання земельної ділянки</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="FullNameType">
		<xsd:annotation>
			<xsd:documentation>Прізвище, ім’я, по батькові фізичної особи</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="LastName" type="NonEmptyString">
				<xsd:annotation>
					<xsd:documentation>Прізвище фізичної особи</xsd:documentation>
					<xsd:appinfo>Прізвище</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FirstName" type="NonEmptyString">
				<xsd:annotation>
					<xsd:documentation>Ім'я фізичної особи</xsd:documentation>
					<xsd:appinfo>Ім'я</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MiddleName" type="NonEmptyString" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>По батькові фізичної особи</xsd:documentation>
					<xsd:appinfo>По батькові ⋮</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="QualificationType">
		<xsd:annotation>
			<xsd:documentation>Кваліфікаційне свідоцтво інженера</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="QualificationNumber" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Номер кваліфікаційного сертифіката</xsd:documentation>
					<xsd:appinfo>Сертифікат №</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="QualificationDate" type="xsd:date">
				<xsd:annotation>
					<xsd:documentation>Дата видачі кваліфікаційного сертифіката</xsd:documentation>
					<xsd:appinfo>Дата</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
		</xsd:all>
	</xsd:complexType>
	<xsd:complexType name="Address">
		<xsd:annotation>
			<xsd:documentation>Адреса ⋮</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Country" default="804">
				<xsd:annotation>
					<xsd:documentation>Код країни згідно з Класифікатором країн світу ДК007-96</xsd:documentation>
					<xsd:appinfo>Країна</xsd:appinfo>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="[0-9]{3}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ZIP" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Поштовий індекс</xsd:documentation>
					<xsd:appinfo>ZIP</xsd:appinfo>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:pattern value="[0-9]{5}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Region" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назва регіону</xsd:documentation>
					<xsd:appinfo>Регіон</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="District" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назва району регіону</xsd:documentation>
					<xsd:appinfo>Район ⋮</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Settlement" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Назва села, селища, міста</xsd:documentation>
					<xsd:appinfo>Населений пункт ⋮</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Street" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Вулиця (проспект, провулок тощо)</xsd:documentation>
					<xsd:appinfo>Вулиця</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Building" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Номер будинку</xsd:documentation>
					<xsd:appinfo>Номер будинку</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Block" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер корпусу</xsd:documentation>
					<xsd:appinfo>Номер корпусу ⋮</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BuildingUnit" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Номер квартири (офіса)</xsd:documentation>
					<xsd:appinfo>Номер квартири</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ExecutorInfo">
		<xsd:annotation>
			<xsd:documentation>Відомості про виконавця робіт</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CompanyName" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Виконавець робіт</xsd:documentation>
					<xsd:appinfo>Назва</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice minOccurs="1">
				<xsd:element name="EDRPOU">
					<xsd:annotation>
						<xsd:documentation>Ідентифікаційний код згідно з ЄДРПОУ</xsd:documentation>
						<xsd:appinfo>EDRPOU</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{8}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="TaxNumber">
					<xsd:annotation>
						<xsd:documentation>Ідентифікаційний номер згідно з ДРФО (за наявності)</xsd:documentation>
						<xsd:appinfo>Ідентифікаційний номер ⋮</xsd:appinfo>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:pattern value="[0-9]{10}"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="License" minOccurs="0" maxOccurs="2">
				<xsd:annotation>
					<xsd:documentation>Інформація про ліцензії на право проведення робіт</xsd:documentation>
					<xsd:appinfo>Ліцензія ⋮</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="LicenseSeries" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Серія ліцензії</xsd:documentation>
								<xsd:appinfo>Серія</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LicenseNumber" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>Номер ліцензії</xsd:documentation>
								<xsd:appinfo>Номер</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="LicenseIssuedDate" type="xsd:date">
							<xsd:annotation>
								<xsd:documentation>Дата видачі ліцензії</xsd:documentation>
								<xsd:appinfo>Дата видачі ↓</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Chief" minOccurs="1" maxOccurs="2">
				<xsd:annotation>
					<xsd:documentation>Дані про відповідальну особу</xsd:documentation>
					<xsd:appinfo>Відповідальна особа ⋮</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ChiefName" type="FullNameType">
							<xsd:annotation>
								<xsd:documentation>Прізвище, ім'я, по батькові відповідальної особи</xsd:documentation>
								<xsd:appinfo>ПІБ</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ChiefPosition" type="NonEmptyString">
							<xsd:annotation>
								<xsd:documentation>Посада відповідальної особи</xsd:documentation>
								<xsd:appinfo>Посада</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Qualification" type="QualificationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Дані кваліфікаційного сертифіката</xsd:documentation>
								<xsd:appinfo>Сертифікат</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Executor" minOccurs="1" maxOccurs="2">
				<xsd:annotation>
					<xsd:documentation>Дані про безпосереднього виконавця</xsd:documentation>
					<xsd:appinfo>Виконавець ⋮</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="ExecutorName" type="FullNameType">
							<xsd:annotation>
								<xsd:documentation>Прізвище, ім'я, по батькові виконавця</xsd:documentation>
								<xsd:appinfo>ПІБ</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ExecutorPosition" type="NonEmptyString">
							<xsd:annotation>
								<xsd:documentation>Посада виконавця</xsd:documentation>
								<xsd:appinfo>Посада</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Qualification" type="QualificationType">
							<xsd:annotation>
								<xsd:documentation>Дані кваліфікаційного сертифіката</xsd:documentation>
								<xsd:appinfo>Сертифікат</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ContactInfo">
							<xsd:annotation>
								<xsd:documentation>Контактна інформація</xsd:documentation>
								<xsd:appinfo>Контактна інформація ⋮</xsd:appinfo>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Phone" type="xsd:string" minOccurs="1" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Номер телефону</xsd:documentation>
											<xsd:appinfo>Телефон</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Fax" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Номер факсу</xsd:documentation>
											<xsd:appinfo>Факс</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Email" minOccurs="0" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Електронна адреса виконавця</xsd:documentation>
											<xsd:appinfo>Email</xsd:appinfo>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:pattern value="([\.a-zA-Z0-9_\-])+@([a-zA-Z0-9_\-]+\.)+[a-zA-Z]{2,6}"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Address" type="Address">
				<xsd:annotation>
					<xsd:documentation>Місцезнаходження (місце проживання)</xsd:documentation>
					<xsd:appinfo>Адреса ⋮</xsd:appinfo>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PointInfo">
		<xsd:annotation>
			<xsd:documentation>Координати вузлів полігона кадастрової одиниці</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Point" minOccurs="3" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Вузол полігона кадастрової одиниці</xsd:documentation>
					<xsd:appinfo>Вузол</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:all>
						<xsd:element name="UIDP" type="xsd:int" default="-1">
							<xsd:annotation>
								<xsd:documentation>Унікальний номер вузла полігона кадастрової одиниці</xsd:documentation>
								<xsd:appinfo>UIDP</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="PN" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Ім’я вузла полігона кадастрової одиниці</xsd:documentation>
								<xsd:appinfo>Ім’я</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DeterminationMethod">
							<xsd:annotation>
								<xsd:documentation>Метод визначення</xsd:documentation>
								<xsd:appinfo>Метод визначення ⋮</xsd:appinfo>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:choice>
									<xsd:element name="Survey" type="xsd:string" fixed="">
										<xsd:annotation>
											<xsd:documentation>Геодезичні виміри</xsd:documentation>
											<xsd:appinfo>Геодезія</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="GPS" type="xsd:string" fixed="">
										<xsd:annotation>
											<xsd:documentation>GPS зйомка</xsd:documentation>
											<xsd:appinfo>GPS</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Digitization" type="xsd:string" fixed="">
										<xsd:annotation>
											<xsd:documentation>Дигіталізація картографічних матеріалів</xsd:documentation>
											<xsd:appinfo>Дигіталізація</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Photogrammetry" type="xsd:string" fixed="">
										<xsd:annotation>
											<xsd:documentation>Фотограмметричні виміри</xsd:documentation>
											<xsd:appinfo>Фотограмметрія</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
								</xsd:choice>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="X" type="xsd:double" default="0">
							<xsd:annotation>
								<xsd:documentation>X</xsd:documentation>
								<xsd:appinfo>X</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Y" type="xsd:double" default="0">
							<xsd:annotation>
								<xsd:documentation>Y</xsd:documentation>
								<xsd:appinfo>Y</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="H" type="xsd:double" default="0" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>H</xsd:documentation>
								<xsd:appinfo>H</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MX" type="xsd:double" default="0" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>MX</xsd:documentation>
								<xsd:appinfo>MX</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MY" type="xsd:double" default="0" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>MY</xsd:documentation>
								<xsd:appinfo>MY</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="MH" type="xsd:double" default="0" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>MH</xsd:documentation>
								<xsd:appinfo>MH</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Description" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Опис точки</xsd:documentation>
								<xsd:appinfo>Опис</xsd:appinfo>
							</xsd:annotation>
						</xsd:element>
					</xsd:all>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Boundary">
		<xsd:annotation>
			<xsd:documentation>Межа об’єкта</xsd:documentation>
		</xsd:annotation>
		<xsd:all>
			<xsd:element name="Lines">
				<xsd:annotation>
					<xsd:documentation>Блок опису усіх поліліній/ліній об’єкта</xsd:documentation>
					<xsd:appinfo>Лінії ⋮</xsd:appinfo>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Line" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>Опис кожної полілінії/лінії об’єкта</xsd:documentation>
								<xsd:appinfo>Лінія ⋮</xsd:appinfo>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:all>
									<xsd:element name="ULID" type="xsd:int" default="-1">
										<xsd:annotation>
											<xsd:documentation>Унікальний номер полілінії об’єкта</xsd:documentation>
											<xsd:appinfo>ULID</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="FP" type="xsd:int" default="0" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Унікальний номер вузла початку лінії</xsd:documentation>
											<xsd:appinfo>FP</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="TP" type="xsd:int" default="0" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Унікальний номер вузла кінця лінії</xsd:documentation>
											<xsd:appinfo>TP</xsd:appinfo>
										</xsd:annotation>
									</xsd:element>
								</xsd:all>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
