										<xsd:element name="Calculation">
											<xsd:annotation>
												<xsd:documentation>Переобчислення</xsd:documentation>
												<xsd:appinfo>Переобчислення</xsd:appinfo>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence>
													<xsd:element ref="CoordinateSystem"/>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:choice>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Error" type="xsd:double" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Похибка</xsd:documentation>
						<xsd:appinfo>Похибка</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Externals">
