									<xsd:documentation>Початок терміну дії права</xsd:documentation>
									<xsd:appinfo>Початок терміну</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ExpirationDate" type="xsd:date" minOccurs="0">
