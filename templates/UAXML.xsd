									<xsd:appinfo>Закінчення терміну</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="Grantee" minOccurs="0" maxOccurs="unbounded">
