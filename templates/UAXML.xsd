						<xsd:documentation>Відомості про відмітку на державному акті</xsd:documentation>
						<xsd:appinfo>Відмітка</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="NotaryMark">
								<xsd:annotation>
									<xsd:documentation>Відмітка нотаріуса</xsd:documentation>
									<xsd:appinfo>Відмітка нотаріуса</xsd:appinfo>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="NotaryName">
