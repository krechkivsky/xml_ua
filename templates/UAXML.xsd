					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="BookNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Обліковий номер документації</xsd:documentation>
									<xsd:appinfo>Обліковий номер</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RegistrationDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата реєстрації</xsd:documentation>
									<xsd:appinfo>Дата реєстрації</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RegistrationAuthority" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Орган, що зареєстрував документацію</xsd:documentation>
									<xsd:appinfo>Орган, що зареєстрував</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="RegistrationCard" minOccurs="0" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Відмітка щодо видачі реєстраційної картки</xsd:documentation>
						<xsd:appinfo>Відмітка</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="BookNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Обліковий номер</xsd:documentation>
									<xsd:appinfo>Обліковий номер</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="IssuanceDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата видачі реєстраційної картки</xsd:documentation>
									<xsd:appinfo>Дата видачі</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="DocumentList" type="DocumentList" minOccurs="3" maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>Перелік документів, які використовуються для формування обмінного файлу</xsd:documentation>
						<xsd:appinfo>Документ ↓</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Expertise" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Відомості про проходження державної експертизи землевпорядної документації</xsd:documentation>
						<xsd:appinfo>Експертиза</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ExpertiseRequired" type="ExpertiseRequired">
								<xsd:annotation>
									<xsd:documentation>Потреба у проведенні державної експертизи</xsd:documentation>
									<xsd:appinfo>Потреба експертизи</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ExpertiseAuthority" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Назва органу Держкомзему</xsd:documentation>
									<xsd:appinfo>Назва органу Держкомзему</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ExpertOpinion" type="ExpertOpinion">
								<xsd:annotation>
									<xsd:documentation>Висновок державної експертизи землевпорядної документації</xsd:documentation>
									<xsd:appinfo>Висновок експертизи</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ExpertiseDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата висновку державної експертизи землевпорядної документації</xsd:documentation>
									<xsd:appinfo>Дата висновку</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ExpertiseNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Номер висновку державної експертизи землевпорядної документації</xsd:documentation>
									<xsd:appinfo>Номер висновку</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ApprovalInfo" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Затвердження документації</xsd:documentation>
						<xsd:appinfo>Затвердження</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ApprovalAuthority" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Назва органу, який затвердив</xsd:documentation>
									<xsd:appinfo>Орган, який затвердив</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата прийняття</xsd:documentation>
									<xsd:appinfo>Дата</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalNumber" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Номер</xsd:documentation>
									<xsd:appinfo>Номер</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ApprovalDocument" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Найменування документа</xsd:documentation>
									<xsd:appinfo>Найменування документа</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="ApprovalActDate" type="xsd:date" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>Дата акта приймання-передачі</xsd:documentation>
						<xsd:appinfo>Дата акта</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="StateActInfo">
		<xsd:annotation>
			<xsd:documentation>Державний акт на земельну ділянку</xsd:documentation>
			<xsd:appinfo>Державний акт ⋮</xsd:appinfo>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="StateActType" type="StateActType">
					<xsd:annotation>
						<xsd:documentation>Тип державного акта</xsd:documentation>
						<xsd:appinfo>Тип ↓</xsd:appinfo>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="StateActForm">
					<xsd:annotation>
						<xsd:documentation>Бланк державного акта</xsd:documentation>
						<xsd:appinfo>Бланк</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:all>
							<xsd:element name="Series" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>Серія</xsd:documentation>
									<xsd:appinfo>Серія</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="Number">
								<xsd:annotation>
									<xsd:documentation>Номер</xsd:documentation>
									<xsd:appinfo>Номер</xsd:appinfo>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[0-9]{6}"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
						</xsd:all>
					</xsd:complexType>
				</xsd:element>
				<xsd:element name="StateActRegistrationInfo">
					<xsd:annotation>
						<xsd:documentation>Реєстрація державного акта</xsd:documentation>
						<xsd:appinfo>Реєстрація</xsd:appinfo>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="RegistrationBookNumber" type="xsd:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>Номер книги реєстрації</xsd:documentation>
									<xsd:appinfo>Номер книги реєстрації</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="SectionNumber">
								<xsd:annotation>
									<xsd:documentation>Номер розділу книги реєстрації</xsd:documentation>
									<xsd:appinfo>Номер розділу</xsd:appinfo>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[0-9]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="RegistrationNumber">
								<xsd:annotation>
									<xsd:documentation>Реєстраційний номер державного акта в книзі реєстрації</xsd:documentation>
									<xsd:appinfo>Реєстраційний номер</xsd:appinfo>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:pattern value="[0-9]+"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="RegistrationDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Дата реєстрації</xsd:documentation>
									<xsd:appinfo>Дата реєстрації</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="RegistrationPersonInfo">
								<xsd:annotation>
									<xsd:documentation>Відомості про осіб, які підписали та зареєстрували державний акт</xsd:documentation>
