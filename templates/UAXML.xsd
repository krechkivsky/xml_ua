									<xsd:documentation>Розмір суборендної плати за земельну ділянку або її частину у грошовій формі</xsd:documentation>
									<xsd:appinfo>Грошова форма</xsd:appinfo>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="OtherSubrent" type="xsd:string" minOccurs="0" maxOccurs="1">
