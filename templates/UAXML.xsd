								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="RegistrationAuthorityName" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Найменування територіального органу Держкомзему</xsd:documentation>
												<xsd:appinfo>Найменування органу</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RegistrationAuthorityDate" type="xsd:date">
											<xsd:annotation>
												<xsd:documentation>Дата відмітки</xsd:documentation>
												<xsd:appinfo>Дата відмітки</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="RegistrationAuthorityNumber" type="xsd:string">
											<xsd:annotation>
												<xsd:documentation>Номер відмітки територіального органу Держкомзему</xsd:documentation>
												<xsd:appinfo>Номер відмітки</xsd:appinfo>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="OwnerName" maxOccurs="unbounded">
											<xsd:annotation>
												<xsd:documentation>Відомості про осіб, які набувають (набули) право власності на земельну ділянку</xsd:documentation>
												<xsd:appinfo>Набувач права власності</xsd:appinfo>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:choice>
													<xsd:element name="NaturalPerson" type="NaturalPersonInfo">
														<xsd:annotation>
															<xsd:documentation>Дані про фізичну особу</xsd:documentation>
															<xsd:appinfo>Фізична особа</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="LegalEntity" type="LegalEntityInfo">
														<xsd:annotation>
															<xsd:documentation>Дані про юридичну особу</xsd:documentation>
															<xsd:appinfo>Юридична особа</xsd:appinfo>
														</xsd:annotation>
													</xsd:element>
												</xsd:choice>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ValuationInfo">
